                            "type": "dict",
                            "schema": {
                                "title": {
                                    "type": "string"
                                },
                                "url": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "allow_unspecified_artifacts": false,
                "is_ordered": true
            },
            "publications": {}
        },
        "discussions": {
            "schema": {
                "required_artifacts": [
                    "discussion.pdf",
                    "solution.pdf"
                ],
                "optional_artifacts": [],
                "metadata_schema": {
                    "number": {
                        "type": "integer"
                    },
                    "topic": {
                        "type": "string"
                    },
                    "date": {
                        "type": "datetime"
                    },
                    "recording": {
                        "type": "string",
                        "required": false,
                        "nullable": true
                    }
                },
                "allow_unspecified_artifacts": false,
                "is_ordered": false
            },
            "publications": {}
        },
        "homeworks": {
            "schema": {
                "required_artifacts": [
                    "homework.pdf",
                    "solution.pdf",
                    "template.zip"
                ],
                "optional_artifacts": [],
                "metadata_schema": {
                    "number": {
                        "type": "integer"
                    },
                    "topic": {
                        "type": "string"
                    },
                    "released": {
                        "type": "smartdatetime"
                    },
                    "due": {
                        "type": "smartdatetime"
                    }
                },
                "allow_unspecified_artifacts": false,
                "is_ordered": false
            },
            "publications": {}
        },
        "default": {
            "schema": {
                "required_artifacts": [],
                "optional_artifacts": null,
                "metadata_schema": null,
                "allow_unspecified_artifacts": true,
                "is_ordered": false
