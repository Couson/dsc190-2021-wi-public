{
    "collections": {
        "lectures": {
            "schema": {
                "required_artifacts": [
                    "slides-eldridge.pdf",
                    "slides-wang.pdf",
                    "slides-wang.pptx"
                ],
                "optional_artifacts": [],
                "metadata_schema": {
                    "number": {
                        "type": "integer"
                    },
                    "date": {
                        "type": "smartdate"
                    },
                    "topic": {
                        "type": "string"
                    },
                    "reading": {
                        "type": "string"
                    },
                    "wang-recording": {
                        "type": "string",
                        "nullable": true
                    },
                    "eldridge-videos": {
                        "type": "list",
